/* components/xing-editor.wxss */
@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1533786095869'); /* IE9*/
  src: url('iconfont.eot?t=1533786095869#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
  url('iconfont.ttf?t=1533786095869') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('iconfont.svg?t=1533786095869#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-image:before { content: "\e7de"; }

.icon-align-left:before { content: "\e7f5"; }

.icon-plus:before { content: "\e7fe"; }

.editor-wrapper {
  position: fixed;
  top: 0;
  left: 0;
  width: 750rpx;
  background: #F2F6FC;
}

.editor-add-wrapper {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 170rpx;
  margin: 10rpx auto;
}

.editor-add {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 75rpx;
  height: 75rpx;
  border: dashed 4rpx #C0C4CC;
  color: #C0C4CC;
  box-sizing: border-box;
}